package perfois;

public class CompleteString {
	public static void main(String[] args) {
		String str = "qwertyu";
		boolean status=true;
		for(int i=97;i<=122;i++) {
			char ch=(char)i;
			System.out.println("=========="+ch);

			if(!str.contains(ch+"")) {
				status=false;
				break;
			}
		}
		
		System.out.println(status);
	}
}
