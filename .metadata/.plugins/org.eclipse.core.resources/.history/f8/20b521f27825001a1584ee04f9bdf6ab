package brillio;

public class ThreadSequence implements Runnable {

	public static void main(String[] args) {

		ThreadSequence r= new ThreadSequence();
		
		Thread t1 = new Thread(r);
		
	}

	@Override
	public void run() {
		
//		while(true) {
//			
//		}
		System.out.println(Thread.currentThread().getName());
		
	}

}
