package fidelity;

public class OddEven implements Runnable{

	int count=0;
	public static void main(String[] args) {
		
		
		
	}

	@Override
	public void run() {
		
		synchronized (this) {
			if(count%2==0) {
				System.out.println(count+"====even====");
			}else {
				System.out.println(count+"====odd====");
			}
		}
		
	}

}
