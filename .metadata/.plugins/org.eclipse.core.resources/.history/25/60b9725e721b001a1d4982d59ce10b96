package collections.concurrent;

import java.util.ArrayList;
import java.util.List;

public class ArrayListIssue {
	public static void main(String[] args) {
		
		
		List<String> arr= new ArrayList<String>();
		arr.add("Apple");
		arr.add("Boy");
		arr.add("Cat");
		arr.add("Dog");
		arr.add("Elephant");
	}
}
